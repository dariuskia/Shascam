B;AAqCjCW,EAAAA,YAAY,EAAE;AAEVqC,IAAAA,UAAU,EAAE,KAFF;AAGVX,IAAAA,QAAQ,EAAE,EAHA;AAIVD,IAAAA,SAAS,EAAE,QAJD;AAKVD,IAAAA,KAAK,EAAE;AALG,GArCmB;AA4CjCN,EAAAA,MAAM,EAAE;AAEJmB,IAAAA,UAAU,EAAE,KAFR;AAGJrE,IAAAA,eAAe,EAAE;AAHb,GA5CyB;AAiDjCyE,EAAAA,UAAU,EAAE;AACRF,IAAAA,SAAS,EAAE,CAAC;AAAEG,MAAAA,UAAU,EAAE;AAAd,KAAD,EAAyB;AAAEF,MAAAA,UAAU,EAAE;AAAd,KAAzB;AADH;AAjDqB,CAAlB,CAAnB;AAqDA,eAAepG,aAAf","sourcesContent":["import React, { Component } from 'react';\r\nimport { Alert, Button, TextInput, View, StyleSheet, Text, TouchableOpacity, SafeAreaView, Image, Dimensions, ScrollView, StatusBar } from 'react-native';\r\nimport AsyncStorage from '@react-native-async-storage/async-storage';\r\n//import Carousel from 'react-native-snap-carousel';\r\nimport Carousel, { Pagination } from 'react-native-snap-carousel';\r\n\r\nimport { Picker } from \"@react-native-picker/picker\";\r\nimport { customstyles } from \"../customstyle\";\r\nimport { getGeneralOffer } from './functions';\r\nconst slide1 = require('../assets/Images/McDonalds-coupons.jpg');\r\n\r\nconst SLIDER_WIDTH = Dimensions.get('window').width - 40;\r\nconst SLIDER_HEIGHT = Math.round(SLIDER_WIDTH * 3 / 4) - 30;\r\nconst ITEM_WIDTH = Math.round(SLIDER_WIDTH) - 60;\r\nconst ITEM_HEIGHT = Math.round(SLIDER_HEIGHT);\r\n\r\n//alert(SLIDER_WIDTH)\r\n//alert(SLIDER_HEIGHT)\r\n\r\nclass ProfileScreen extends Component {\r\n    state = {\r\n        userData: '',\r\n        activeIndex: 0,\r\n        carouselItems: [],\r\n        // carouselItems: [\r\n        //     {\r\n        //         title: \"Item 1\",\r\n        //         text: \"Text 1\",\r\n        //     },\r\n        //     {\r\n        //         title: \"Item 2\",\r\n        //         text: \"Text 2\",\r\n        //     },\r\n        //     {\r\n        //         title: \"Item 3\",\r\n        //         text: \"Text 3\",\r\n        //     },\r\n        //     {\r\n        //         title: \"Item 4\",\r\n        //         text: \"Text 4\",\r\n        //     },\r\n        //     {\r\n        //         title: \"Item 5\",\r\n        //         text: \"Text 5\",\r\n        //     },\r\n        // ]\r\n    }\r\n    componentDidMount() {\r\n        const value = AsyncStorage.getItem('userInfo')\r\n            .then((value) => {\r\n                //console.log(JSON.parse(value));\r\n                this.setState({\r\n                    userData: JSON.parse(value),\r\n                });\r\n            });\r\n        this.getGeneralOffer();\r\n    }\r\n\r\n    onPressEditProfile = () => {\r\n        this.props.navigation.navigate('EditProfile', {\r\n            userDetails: this.state.userData,\r\n        })\r\n    }\r\n\r\n    onPressCategory = () => {\r\n        this.props.navigation.navigate('Category', {\r\n            userDetails: this.state.userData,\r\n        })\r\n    }\r\n\r\n    userLogout = () => {\r\n        AsyncStorage.removeItem('userInfo');\r\n        this.props.navigation.navigate('LoginScreen');\r\n    }\r\n\r\n    // Get General offer\r\n\r\n    getGeneralOffer = () => {\r\n        var resp = getGeneralOffer()\r\n            .then(resp => {\r\n                let result = resp.data;\r\n                //console.log(result);\r\n                this.setState({\r\n                    carouselItems: result,\r\n                })\r\n            })\r\n            .catch((error) => {\r\n                console.log(error)\r\n            })\r\n    }\r\n    // Close\r\n\r\n    _renderItem({ item, index }) {    // coursel\r\n        return (\r\n            <View style={{\r\n                backgroundColor: 'floralwhite',\r\n                borderRadius: 5,\r\n                height: ITEM_HEIGHT,\r\n                //height: 400,\r\n                padding: 0,\r\n                marginLeft: 0,\r\n                marginRight: 0,\r\n                borderRadius: 10,\r\n                overflow: \"hidden\"\r\n            }}>\r\n                <Image source={slide1} style={styles.sliderImg} />\r\n                <Text style={styles.company_name}>{item.comp_name}</Text>\r\n                <Text style={styles.offerTitleHeading}>{item.offer_title}</Text>\r\n                <Text style={styles.price}>$ {item.data_share_price + item.open_to_contact_price}</Text>\r\n            </View>\r\n\r\n        )\r\n    }\r\n\r\n    render() {\r\n        return (\r\n\r\n            <SafeAreaView style={styles.scrollArea}>\r\n                <ScrollView style={styles.innerView}>\r\n                    <View style={styles.container}>\r\n\r\n                        <View style={customstyles.dropdownBox}>\r\n                            <Picker\r\n\r\n                                style={{ height: 50, width: 150 }}\r\n                            >\r\n                                <Picker.Item>\r\n                                    <TouchableOpacity>\r\n                                        <Text style={customstyles.textJoin}>Edit Profile</Text>\r\n                                    </TouchableOpacity>\r\n                                </Picker.Item>\r\n\r\n                            </Picker>\r\n                        </View>\r\n\r\n                        <View style={{ marginBottom: 20 }}>\r\n                            <TouchableOpacity\r\n                                onPress={this.onPressEditProfile}>\r\n                                <Text>Edit Profile</Text>\r\n                            </TouchableOpacity>\r\n\r\n                            <TouchableOpacity\r\n                                onPress={this.onPressCategory}>\r\n                                <Text>Categories</Text>\r\n                            </TouchableOpacity>\r\n\r\n                            <TouchableOpacity\r\n                                onPress={this.userLogout}>\r\n                                <Text>Logout</Text>\r\n                            </TouchableOpacity>\r\n                        </View>\r\n\r\n                        <View>\r\n                            <Text style={customstyles.titleText}>Welcome {this.state.userData.fname != '' ? this.state.userData.fname : ''} {this.state.userData.lname != '' ? this.state.userData.lname : ''}</Text>\r\n                            {/* <Text style={customstyles.subText}>{this.state.userData.fname != '' ? this.state.userData.fname : ''}</Text> */}\r\n                            {/* <Text style={customstyles.subText}>{this.state.userData.lname != '' ? this.state.userData.lname : ''}</Text> */}\r\n                        </View>\r\n                        <View>\r\n                            <Text style={customstyles.update}>Here are your updates!</Text>\r\n                        </View>\r\n                        <View style={[styles.row, { marginBottom: 20, }]}>\r\n                            <View style={[customstyles.whitebox, { width: \"46%\" }]}>\r\n                                <TouchableOpacity\r\n                                    onPress={this.onPressInfo}\r\n                                    style={storeStyle.widgetStore}>\r\n                                    <Text style={storeStyle.titleText}>Info Store</Text>\r\n\r\n                                    <Text style={storeStyle.completionText}>Progress</Text>\r\n\r\n                                    <View style={{ width: \"50%\", borderRadius: 10, marginLeft: \"auto\", marginRight: \"auto\", marginBottom: 10 }}>\r\n                                        {/* <AnimatedProgressProvider\r\n                                    valueStart={0}\r\n                                    valueEnd={this.state.progressPercent}\r\n                                    duration={6}\r\n                                    easingFunction={easeQuadInOut}\r\n                                >\r\n                                    {value => {\r\n                                        const roundedValue = Math.round(value);\r\n                                        return (\r\n                                            <CircularProgressbar\r\n                        