;\r\n    }\r\n\r\n    onPressEditProfile = () => {\r\n        this.props.navigation.navigate('EditProfile', {\r\n            userDetails: this.state.userData,\r\n        })\r\n    }\r\n\r\n    onPressCategory = () => {\r\n        this.props.navigation.navigate('Category', {\r\n            userDetails: this.state.userData,\r\n        })\r\n    }\r\n\r\n    userLogout = () => {\r\n        AsyncStorage.removeItem('userInfo');\r\n        this.props.navigation.navigate('LoginScreen');\r\n    }\r\n\r\n    // Get General offer\r\n\r\n    getGeneralOffer = () => {\r\n        var resp = getGeneralOffer()\r\n            .then(resp => {\r\n                let result = resp.data;\r\n                //console.log(result);\r\n                this.setState({\r\n                    carouselItems: result,\r\n                })\r\n            })\r\n            .catch((error) => {\r\n                console.log(error)\r\n            })\r\n    }\r\n    // Close\r\n\r\n    _renderItem({ item, index }) {    // coursel\r\n        return (\r\n            <View style={{\r\n                backgroundColor: 'floralwhite',\r\n                borderRadius: 5,\r\n                height: 250,\r\n                padding: 50,\r\n                marginLeft: 25,\r\n                marginRight: 25,\r\n            }}>\r\n                <Image source={slide1} style={{ height: 200, width: '100%' }} />\r\n                <Text style={{ fontSize: 20 }}>{item.comp_name}</Text>\r\n                <Text style={{ fontSize: 15 }}>{item.offer_title}</Text>\r\n                <Text>$ {item.data_share_price + item.open_to_contact_price}</Text>\r\n            </View>\r\n\r\n        )\r\n    }\r\n\r\n\r\n    render() {\r\n        return (\r\n            <SafeAreaView style={styles.container}>\r\n\r\n                <Text>Welcome </Text>\r\n                <Text>{this.state.userData.fname != '' ? this.state.userData.fname : ''}</Text>\r\n                <Text>{this.state.userData.lname != '' ? this.state.userData.lname : ''}</Text>\r\n                <View>\r\n                    <Text>Here are your updates!</Text>\r\n                </View>\r\n\r\n                <View style={{ marginBottom: 20 }}>\r\n                    <TouchableOpacity\r\n                        onPress={this.onPressEditProfile}>\r\n                        <Text>Edit Profile</Text>\r\n                    </TouchableOpacity>\r\n\r\n                    <TouchableOpacity\r\n                        onPress={this.onPressCategory}>\r\n                        <Text>Categories</Text>\r\n                    </TouchableOpacity>\r\n\r\n                    <TouchableOpacity\r\n                        onPress={this.userLogout}>\r\n                        <Text>Logout</Text>\r\n                    </TouchableOpacity>\r\n                </View>\r\n\r\n                {/* coursel */}\r\n                <View style={{ flex: 1, flexDirection: 'row', justifyContent: 'center', }}>\r\n                    <Carousel\r\n                        layout={\"default\"}\r\n                        ref={ref => this.carousel = ref}\r\n                        data={this.state.carouselItems}\r\n                        sliderWidth={300}\r\n                        itemWidth={300}\r\n                        renderItem={this._renderItem}\r\n                        onSnapToItem={index => this.setState({ activeIndex: index })} />\r\n                </View>\r\n                {/* Close */}\r\n\r\n            </SafeAreaView >\r\n        );\r\n    }\r\n}\r\n\r\nconst styles = StyleSheet.create({\r\n    container: {\r\n        flex: 1,\r\n        alignItems: 'center',\r\n        justifyContent: 'center',\r\n        backgroundColor: '#ecf0f1',\r\n    }\r\n});\r\n\r\nexport default Dashboard;"]},"metadata":{},"sourceType":"module"}